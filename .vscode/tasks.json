{
	"version": "2.0.0",
	"tasks": [
		{
			"type": "cmake",
			"label": "CMake: build",
			"command": "build",
			"targets": [
				"rust-cef-demo-bundle"
			],
			"group": {
				"kind": "build",
				"isDefault": true
			},
			"dependsOn": [
				"CMake: install"
			],
			"problemMatcher": [],
			"detail": "CMake template build task"
		},
		{
			"type": "shell",
			"label": "CMake: install",
			"command": "cmake",
			"args": [
				"-B ./build",
				"-S .",
				"-G \"Ninja Multi-Config\""
			],
			"options": {
				"cwd": "${workspaceFolder}"
			},
			"problemMatcher": [],
			"detail": "CMake template install task"
		}
	]
}
